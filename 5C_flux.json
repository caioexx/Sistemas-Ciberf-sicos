        "id": "e8efd678f1d931ff",
        "type": "function",
        "z": "ae590a841509cede",
        "name": "function 16",
        "func": "// 1. Acessa o objeto de parâmetros que veio do MQTT\nconst params = msg.payload.parameters;\n\n// Se 'params' não existir, pare o fluxo para evitar erros\nif (!params) {\n    node.warn(\"msg.payload.parameters está vazio ou não existe.\");\n    return null; \n}\n\n// 2. Função auxiliar para arredondar números\nfunction round(value) {\n    if (typeof value === 'number') {\n        return value.toFixed(2);\n    }\n    return value; // Retorna valores não-numéricos (como \"Running\")\n}\n\n// --- MENSAGENS DE SAÍDA ---\n// Esta função está configurada para 7 saídas.\n// Vamos criar 7 objetos de mensagem, um para cada saída.\n\n// Saída 1: Tabela de Posição (para Group 2)\nconst posData = [\n    { \"Parâmetro\": \"Posição X\", \"Valor\": round(params.positionX) },\n    { \"Parâmetro\": \"Posição Y\", \"Valor\": round(params.positionY) },\n    { \"Parâmetro\": \"Posição Z\", \"Valor\": round(params.positionZ) },\n    { \"Parâmetro\": \"Rotação X\", \"Valor\": round(params.positionRX) },\n    { \"Parâmetro\": \"Rotação Y\", \"Valor\": round(params.positionRY) },\n    { \"Parâmetro\": \"Rotação Z\", \"Valor\": round(params.positionRZ) }\n];\nconst msgPos = { payload: posData };\n\n// Saída 2: Texto de Status (para Group 2)\nconst msgStatus = { payload: params.status };\n\n// Saída 3: Tabela de Juntas (para Group 3)\nconst jointsData = [\n    { \"Junta\": \"Junta 1 (Base)\", \"Valor\": round(params.joint1) },\n    { \"Junta\": \"Junta 2 (Shoulder)\", \"Valor\": round(params.joint2) },\n    { \"Junta\": \"Junta 3 (Elbow)\", \"Valor\": round(params.joint3) },\n    { \"Junta\": \"Junta 4 (Wrist 1)\", \"Valor\": round(params.joint4) },\n    { \"Junta\": \"Junta 5 (Wrist 2)\", \"Valor\": round(params.joint5) },\n    { \"Junta\": \"Junta 6 (Wrist 3)\", \"Valor\": round(params.joint6) }\n];\nconst msgJoints = { payload: jointsData };\n\n// Saída 4: Gráfico de Temperatura (para Group 4)\n// Gráficos 'ui_chart' esperam apenas o número no payload\nconst msgTemp = { payload: round(params.temperature) }; \n\n// Saída 5: Gráfico de Tensão (para Group 4)\nconst msgVolt = { payload: round(params.voltage) };\n\n// Saída 6: Gráfico de Corrente (para Group 4)\nconst msgCurr = { payload: round(params.current) };\n\n// Saída 7: Gráfico de Corrente do Robot (para Group 4)\nconst msgRobCurr = { payload: round(params.robotCurrent) };\n\n// 3. Retorna todas as mensagens como um array\n// A ordem no array corresponde à ordem das saídas (de 1 a 7)\nreturn [ \n    msgPos, \n    msgStatus, \n    msgJoints, \n    msgTemp, \n    msgVolt, \n    msgCurr, \n    msgRobCurr \n];",
